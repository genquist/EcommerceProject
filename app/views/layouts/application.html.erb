<!DOCTYPE html>
<html>
<head>
  <title>EcommerceProject</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	
<%= render 'layouts/shopping_cart' unless @disable_cart %>
<div  class="container">
	<%= render 'layouts/header' %>
	<%= render 'layouts/nav' %>
	<section id="content">
		<%= yield %>
	</section>
	<footer></footer>
</div>

</body>
</html>
